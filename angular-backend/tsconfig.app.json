{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "@Lortom/*": [
        "*"
      ],
      "@Lortom-Backend/*": [
        "app/backend-module/*"
      ],
      "@LortomPlugins/*": [
        "plugins"
      ],
    },
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
