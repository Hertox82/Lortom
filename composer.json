{
    "name": "hadeluca/lortom",
    "description": "Lortom is a CMS based on Laravel Framework, The Backend was built with Angular 8",
    "keywords": [
        "cms",
        "laravel",
        "angular8",
        "laravel plus angular",
        "angular 8"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Hernan Ariel De Luca",
            "email": "hadeluca@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "hadeluca/db-exporter": "^2.4",
        "hadeluca/lortom-framework": "1.5.*",
        "laravel/framework": "^7.29",
        "laravel/tinker": "^2.5"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Plugins\\": "angular-backend/src/plugins/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "hadeluca/db-exporter"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('ltpm.config.json') || copy('ltpm.config.json.example', 'ltpm.config.json');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan lt-setup:init"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
